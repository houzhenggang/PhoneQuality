<?xml version="1.0" encoding="utf-8"?>

<com.cattsoft.phone.quality.ui.widget.OverScrollView xmlns:android="http://schemas.android.com/apk/res/android"
                                                     android:orientation="vertical"
                                                     android:layout_width="match_parent"
                                                     android:layout_height="match_parent">

	<LinearLayout
			android:id="@+id/rootView"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">

		<LinearLayout
				android:id="@+id/chartLayout"
				android:layout_width="fill_parent"
				android:layout_height="248dip"
				android:orientation="vertical"/>

		<LinearLayout
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical">

			<RelativeLayout
					android:layout_width="fill_parent"
					android:layout_height="42.0dip"
					android:gravity="center_vertical">

				<TextView
						android:layout_marginLeft="16.0dip"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="网络运营商"
						android:textColor="@color/text"
						android:layout_gravity="center_vertical"/>

				<TextView
						android:layout_marginRight="16.0dip"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:id="@+id/operator"
						android:textColor="@color/text_inverted"
						android:layout_gravity="center_vertical"/>
			</RelativeLayout>

			<RelativeLayout
					android:layout_width="fill_parent"
					android:layout_height="42.0dip"
					android:gravity="center_vertical">

				<TextView
						android:layout_marginLeft="16.0dip"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:text="设备型号"
						android:textColor="@color/text"
						android:layout_gravity="center_vertical"/>

				<TextView
						android:layout_marginRight="16.0dip"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:id="@+id/deviceModel"
						android:textColor="@color/text_inverted"
						android:layout_gravity="center_vertical"/>
			</RelativeLayout>

			<RelativeLayout
					android:layout_width="fill_parent"
					android:layout_height="42.0dip"
					android:gravity="center_vertical">

				<TextView
						android:layout_marginLeft="16.0dip"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:text="网络类型"
						android:textColor="@color/text"
						android:layout_gravity="center_vertical"/>

				<TextView
						android:layout_marginRight="16.0dip"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:id="@+id/networkType"
						android:textColor="@color/text_inverted"
						android:layout_gravity="center_vertical"/>
			</RelativeLayout>

			<RelativeLayout
					android:layout_width="fill_parent"
					android:layout_height="42.0dip"
					android:gravity="center_vertical">

				<TextView
						android:layout_marginLeft="16.0dip"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:text="数据状态"
						android:textColor="@color/text"
						android:layout_gravity="center_vertical"/>

				<TextView
						android:layout_marginRight="16.0dip"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:id="@+id/networkState"
						android:textColor="@color/text_inverted"
						android:layout_gravity="center_vertical"/>
			</RelativeLayout>

			<RelativeLayout
					android:layout_width="fill_parent"
					android:layout_height="42.0dip"
					android:gravity="center_vertical">

				<TextView
						android:layout_marginLeft="16.0dip"
						android:layout_width="fill_parent"
						android:layout_height="wrap_content"
						android:text="外部IP"
						android:textColor="@color/text"
						android:layout_gravity="center_vertical"/>

				<TextView
						android:layout_marginRight="16.0dip"
						android:layout_alignParentRight="true"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="无法获取"
						android:id="@+id/externalIP"
						android:textColor="@color/text_inverted"
						android:layout_gravity="center_vertical"/>
			</RelativeLayout>
		</LinearLayout>

	</LinearLayout>
</com.cattsoft.phone.quality.ui.widget.OverScrollView>