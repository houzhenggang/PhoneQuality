<resources>

	<style name="Theme.Dark" parent="@style/Theme.AppCompat.Light.DarkActionBar">
		<item name="actionBarItemBackground">@drawable/selectable_background_dark</item>
		<item name="popupMenuStyle">@style/PopupMenu.Dark</item>
		<item name="dropDownListViewStyle">@style/DropDownListView.Dark</item>
		<item name="actionBarTabStyle">@style/ActionBarTabStyle.Dark</item>
		<item name="actionDropDownStyle">@style/DropDownNav.Dark</item>
		<item name="actionBarStyle">@style/ActionBar.Solid.Dark</item>
		<item name="actionModeBackground">@drawable/cab_background_top_dark</item>
		<item name="actionModeSplitBackground">@drawable/cab_background_bottom_dark</item>
		<item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Dark</item>

		<!-- Light.DarkActionBar specific -->
		<item name="actionBarWidgetTheme">@style/Theme.Dark.Widget</item>
		<item name="android:textColor">@color/text_inverted</item>

		<item name="android:windowContentOverlay">@null</item>
		<item name="android:windowBackground">@color/pager_background</item>

	</style>

	<style name="Setting" parent="Theme.Dark">
		<!--<item name="android:background">#FFEAEAEA</item>-->
		<item name="android:windowBackground">@color/white</item>
		<item name="android:textColor">@color/text_dark</item>
	</style>

	<style name="ActionBar.Solid.Dark" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
		<item name="background">@drawable/ab_solid_dark</item>
		<item name="backgroundStacked">@drawable/ab_stacked_solid_dark</item>
		<item name="backgroundSplit">@drawable/ab_bottom_solid_dark</item>
		<item name="progressBarStyle">@style/ProgressBar.Dark</item>
	</style>

	<style name="ActionBar.Transparent.Dark" parent="@style/Widget.AppCompat.ActionBar">
		<item name="background">@drawable/ab_transparent_dark</item>
		<item name="progressBarStyle">@style/ProgressBar.Dark</item>
	</style>

	<style name="PopupMenu.Dark" parent="@style/Widget.AppCompat.PopupMenu">
		<item name="android:popupBackground">@drawable/menu_dropdown_panel_dark</item>
	</style>

	<style name="DropDownListView.Dark" parent="@style/Widget.AppCompat.ListView.DropDown">
		<item name="android:listSelector">@drawable/selectable_background_dark</item>
	</style>

	<style name="ActionBarTabStyle.Dark" parent="@style/Widget.AppCompat.ActionBar.TabView">
		<item name="android:background">@drawable/tab_indicator_ab_dark</item>
	</style>

	<style name="DropDownNav.Dark" parent="@style/Widget.AppCompat.Spinner.DropDown.ActionBar">
		<item name="android:background">@drawable/spinner_background_ab_dark</item>
		<item name="android:popupBackground">@drawable/menu_dropdown_panel_dark</item>
		<item name="android:dropDownSelector">@drawable/selectable_background_dark</item>
	</style>

	<style name="ProgressBar.Dark" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
		<item name="android:progressDrawable">@drawable/progress_horizontal_dark</item>
	</style>

	<style name="ActionButton.CloseMode.Dark" parent="@style/Widget.AppCompat.ActionButton.CloseMode">
		<item name="android:background">@drawable/btn_cab_done_dark</item>
	</style>

	<!-- this style is only referenced in a Light.DarkActionBar based theme -->
	<style name="Theme.Dark.Widget" parent="@style/Theme.AppCompat">
		<item name="popupMenuStyle">@style/PopupMenu.Dark</item>
		<item name="dropDownListViewStyle">@style/DropDownListView.Dark</item>
	</style>

	<style name="Theme.Dark.Splash" parent="@style/Theme.Dark">
		<item name="android:windowActionBar">false</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:windowContentOverlay">@null</item>
		<item name="android:windowBackground">@drawable/splash</item>
	</style>

	<style name="PagerSlidingTabStyle">
		<item name="pstsTabBackground">@drawable/psts_background_tab</item>
		<item name="pstsIndicatorHeight">3.0dip</item>
		<item name="pstsIndicatorColor">#0681ab</item>
		<item name="pstsShouldExpand">true</item>
		<item name="pstsUnderlineHeight">2dip</item>
		<item name="pstsUnderlineColor">#313035</item>
	</style>

	<style name="GenericProgressBackground" parent="android:Theme">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">fill_parent</item>
		<item name="android:gravity">center</item>
	</style>

	<style name="GenericProgressIndicator" parent="@android:style/Widget.ProgressBar.Small">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:indeterminate">true</item>
	</style>

	<style name="NetworkActivityTitle">
		<item name="android:textSize">16.0dip</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">@color/text</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_gravity">center_vertical</item>
		<item name="android:layout_width">0.0dip</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_weight">1</item>
		<item name="android:lines">1</item>
	</style>

	<style name="AppTrafficValue">
		<item name="android:textSize">14.0sp</item>
		<item name="android:ellipsize">end</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_gravity">center_vertical</item>
		<item name="android:layout_width">0.0dip</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_weight">1</item>
		<item name="android:lines">1</item>
	</style>

	<style name="SpeedResultValue">
		<item name="android:textSize">22.0dip</item>
		<item name="android:textStyle">bold</item>
		<item name="android:gravity">center</item>
		<item name="android:layout_gravity">center_vertical</item>
		<item name="android:layout_width">0.0dip</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_weight">1</item>
		<item name="android:lines">1</item>
	</style>

	<style name="SpeedResultDetailValue">
		<item name="android:textSize">23.0dip</item>
		<item name="android:ellipsize">end</item>
		<item name="android:gravity">left</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginLeft">59.0dip</item>
		<item name="android:layout_marginTop">15.0dip</item>
		<item name="android:lines">1</item>
	</style>

	<style name="FileDownloadError">
		<item name="android:textColor">@color/red</item>
	</style>
</resources>